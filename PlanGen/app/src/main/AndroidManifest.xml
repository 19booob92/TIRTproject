<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.pwr.tirt.plangen" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogicActivity"
            android:label="@string/title_activity_logic"
            android:parentActivityName=".TestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.pwr.tirt.plangen.TestActivity" />
        </activity>
        <activity
            android:name=".LayoutActivity"
            android:label="@string/title_activity_layout"
            android:parentActivityName=".TestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.pwr.tirt.plangen.TestActivity" />
        </activity>
    </application>

</manifest>
